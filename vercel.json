{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "routes": [
    {
      "src": "/[^/]+\\.[^/]+$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      }
    }
  ]
} 